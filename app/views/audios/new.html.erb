<h1>ファイルを登録</h1>
<%= form_with(model: @audio, local: true) do |f| %>
  <div class="audio_title">
    <%= f.label :タイトル %>
    <%= f.text_field :titile %>
  </div>
  <div class="audio_bpm">
    <%= f.label :bpm %>
    <%= f.text_field :titile %>
  </div>
  <div class="audio_key">
    <%= f.label :key %>
    <%= f.text_field :titile %>
  </div>
  <div class="audio_comment">
    <%= f.label :comment %>
    <%= f.text_field :comment %>
  </div>
  <%= f.submit %>
<% end %>
