<h1>プロジェクト名</h1>

<table>
  <tr>
    <th>タイトル</th>
    <th>キー</th>
    <th>BPM</th>
  </tr>

<% @audios.each do |audio| %>
  <tr>
    <td><%= audio.title %></td>
    <td><%= audio.key %></td>
    <td><%= audio.bpm %></td>
    <td><%= link_to '編集', audios_edit_path %></td>
  </tr>
<% end %>
</table>

